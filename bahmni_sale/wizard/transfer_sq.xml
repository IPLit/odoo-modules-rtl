<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="sale_order_transfer_sq_wizard_view" model="ir.ui.view">
        <field name="name">sale.order.transfer.sq.wizard.form</field>
        <field name="model">transfer.sale.quotation.wiz</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="shop_id"/>
                </group>
                <footer>
                    <button name="transfer" type="object" string="Transfer" class="btn-primary"/>
                     <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
